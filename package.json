{
    "name": "material-ui-appframe",
    "description": "Responsive application frame for Material UI web applications",
    "version": "0.1.3",
    "main": "index.js",
    "author": "Sysart Oy",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/sysart/material-ui-appframe.git"
    },
    "devDependencies": {
        "@material-ui/core": "^3.0.1",
        "@material-ui/icons": "^3.0.1",
        "@types/classnames": "^2.2.6",
        "@types/expect-puppeteer": "^2.2.4",
        "@types/jest-environment-puppeteer": "^2.2.1",
        "@types/material-ui": "^0.21.5",
        "@types/node": "^10.5.6",
        "@types/puppeteer": "^1.6.3",
        "@types/react": "^16.4.7",
        "@types/react-dom": "^16.0.6",
        "@types/react-router-dom": "^4.3.0",
        "cross-env": "^5.2.0",
        "husky": "^0.14.3",
        "jest": "^23.6.0",
        "jest-puppeteer": "^3.3.1",
        "lint-staged": "^7.2.0",
        "prettier": "^1.14.0",
        "puppeteer": "^1.8.0",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-router-dom": "^4.3.1",
        "react-scripts-ts": "^2.16.0",
        "shelljs": "^0.8.2",
        "ts-jest": "^23.1.4",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.14.0",
        "tslint-plugin-prettier": "^1.3.0",
        "typescript": "^3.0.1",
        "webpack": "^4.16.4"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "returnof": "^1.1.1"
    },
    "peerDependencies": {
        "@material-ui/core": "^3.0.1",
        "@material-ui/icons": "^3.0.1",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-router-dom": "^4.3.1"
    },
    "scripts": {
        "start": "react-scripts-ts start",
        "build": "node build.js",
        "test": "jest",
        "eject": "react-scripts-ts eject",
        "lint": "tslint 'src/**/*.ts*'",
        "format": "prettier --config .prettierrc.json --write 'src/**/*.ts*'",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "*.ts*": [
            "prettier --config .prettierrc.json --write",
            "git add"
        ]
    },
    "jest": {
        "preset": "jest-puppeteer",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "modulePathIgnorePatterns": [
            "/node_modules/",
            "/dist/"
        ],
        "setupTestFrameworkScriptFile": "./jest.setup.js",
        "verbose": true
    }
}
